# Prefer OpenRouter (recommended)
OPENROUTER_API_KEY=your_key_here
# Pick an image-capable model on OpenRouter (example)
OPENROUTER_IMAGE_MODEL=google/gemini-2.5-flash-image
# Optional: text model used for "keywords -> prompt" optimization
OPENROUTER_TEXT_MODEL=openai/gpt-4o-mini
# Optional attribution headers for OpenRouter
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_SITE_TITLE=AI Girls Workshop

# Optional: fallback to OpenAI Images API
OPENAI_API_KEY=
# Optional (default: gpt-image-1)
OPENAI_IMAGE_MODEL=gpt-image-1

# Optional: force provider (openrouter|openai)
AI_IMAGE_PROVIDER=openrouter

# Prompt optimizer provider (openrouter|deepseek)
PROMPT_OPTIMIZER_PROVIDER=openrouter

# DeepSeek (optional, for prompt optimization)
DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_TEXT_MODEL=deepseek-chat
# Compatibility aliases (supported if you already have these env names wired):
# ANTHROPIC_BASE_URL=https://api.deepseek.com
# ANTHROPIC_AUTH_TOKEN=your_deepseek_key
# ANTHROPIC_MODEL=deepseek-chat

# Supabase (DB + Auth)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Server-only (required for username login + profile creation)
SUPABASE_SERVICE_ROLE_KEY=

# Optional: store generated images on Qiniu (S3-compatible)
# If configured, /api/generate will upload images and return public URLs instead of inline data URLs.
QINIU_S3_ACCESS_KEY=
QINIU_S3_SECRET_KEY=
QINIU_S3_REGION=ap-southeast-1
QINIU_S3_ENDPOINT=https://s3.ap-southeast-1.qiniucs.com
QINIU_S3_BUCKET=aigirl666
QINIU_PUBLIC_BASE_URL=https://aigirl666.s3.ap-southeast-1.qiniucs.com
# Optional
QINIU_S3_FORCE_PATH_STYLE=true
QINIU_CACHE_CONTROL=public, max-age=31536000, immutable
# If bucket is private, enable presigned URLs for viewing.
QINIU_BUCKET_PRIVATE=true
# Signed URL TTL (seconds)
QINIU_SIGNED_URL_TTL_SECONDS=3600
# Signing mode for private bucket URLs:
# - (empty): auto-detect (recommended)
# - s3: AWS SigV4 presigned URL against QINIU_S3_ENDPOINT (for *.qiniucs.com / S3-compatible domains)
# - qiniu: ?e=...&token=... (for Qiniu private download domains like *.gdipper.com)
QINIU_SIGNING_MODE=
# If Qiniu provides a dedicated private download domain (recommended for private buckets), set it here:
# Example: http://t8g9utwu7.sabkt.gdipper.com
QINIU_DOWNLOAD_BASE_URL=
